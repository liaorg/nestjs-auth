        "nestjs-i18n": "^9.1.6",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.6",
        "sqlite3": "^5.1.2",
        "stacktrace-js": "^2.0.2",
        "typeorm": "^0.3.10"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.1",
        "@nestjs/testing": "^9.0.11",
        "@types/express": "^4.17.13",
        "@types/jest": "28.1.7",
        "@types/minimist": "^1.2.2",
        "@types/node": "^18.7.11",
