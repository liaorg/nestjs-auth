        "@nestjs/jwt": "^9.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.11",
        "@nestjs/swagger": "^6.0.5",
        "@nestjs/typeorm": "^9.0.1",
        "@types/compression": "^1.7.2",
        "ajv": "^8.11.0",
        "ajv-errors": "^3.0.0",
        "ajv-formats": "^2.1.1",
        "chalk": "^4.1.2",
        "class-transformer": "^0.5.1",
